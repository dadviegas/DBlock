{
  "name": "mblock-components",
  "version": "0.0.85",
  "description": "module to handle react builds",
  "main": "./dist/index.js",
  "scripts": {
    "compile": "babel -d dist/ src/",
    "start": "babel -d lib/ src/ -w",
    "exec": "clear && npm run compile && node lib",
    "clean-lib": "rimraf ./dist",
    "build:run": "webpack",
    "build:development": "cross-env NODE_ENV=development npm run build:run",
    "build": "cross-env NODE_ENV=production npm run build:run"
  },
  "author": "dadviegas",
  "license": "ISC",
  "dependencies": {
    "highlightjs": "^9.10.0",
    "markdown-it": "^8.4.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-fontawesome": "^0.2.0",
    "mblock-react": "^0.0.85"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
