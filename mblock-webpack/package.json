{
  "name": "mblock-webpack",
  "version": "0.0.15",
  "description": "webpack builder",
  "main": "./lib/index.js",
  "scripts": {
    "compile": "babel -d lib/ src/",
    "start": "babel -d lib/ src/ -w",
    "build": "npm run clean-lib && standard && npm run compile",
    "clean-lib": "rimraf ./lib",
    "install": "npm run compile"
  },
  "keywords": [
    "webpack",
    "build"
  ],
  "author": "Dadviegas",
  "license": "ISC",
  "dependencies": {
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "mblock-babel": "latest",
    "mblock-utils": "latest",
    "webpack": "^3.6.0",
    "webpack-merge": "^4.1.0"
  },
  "standard": {
    "ignore": [
      "/lib/"
    ]
  }
}
