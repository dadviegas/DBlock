{
  "name": "mblock-utils-example",
  "version": "0.0.0",
  "description": "test building modules ",
  "main": "lib/index.js",
  "module": "src/index.js",
  "scripts": {
    "clean-lib": "rimraf ./lib ./es",
    "prebuild": "npm run clean-lib",
    "build": "standard && webpack",
    "build:qa": "npm run build -- --env.environment=qa",
    "build:production": "npm run build -- --env.environment=production",
    "build:es": "cross-env BABEL_ENV=es babel --no-babelrc src --out-dir es",
    "postbuild": "node lib",
    "install": "npm run build:es && npm run build:production"
  },
  "author": "dadviegas",
  "license": "ISC",
  "dependencies": {
    "cross-env": "^5.0.5",
    "mblock-module": "latest"
  },
  "standard": {
    "ignore": [
      "/lib/"
    ]
  }
}
